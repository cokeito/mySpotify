<h1>Users#playlists</h1>

<table class="table table-bordered table-hovered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Duration</th>
      <th>Genre</th>
    </tr>
  </thead>

  <tbody>
    <% @user.songs.each do |s| %>
      <tr>
        <td><%= s.name %></td>
        <td><%= s.duration %></td>
        <td><%= s.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>